/*icon.wxss*/
[class^="icon-"],
[class*=" icon-"] {
  position: relative;
  display: inline-block;
  font-family: 'iconfont';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  vertical-align: middle;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-scissors:before { content: "\e600"; }